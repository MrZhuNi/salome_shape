<source>
  <multi_selector id="main_objects"
    label="Main objects"
    icon=""
    tooltip="Select objects"
    type_choice="faces solids compsolids"
    use_choice="false"
    concealment="true">
    <validator id="PartSet_DifferentObjects"/>
    <validator id="FeaturesPlugin_ValidatorBooleanSmashSelection"/>
  </multi_selector>
  <multi_selector id="tool_objects"
    label="Tool objects"
    icon=""
    tooltip="Select tools"
    type_choice="faces solids compsolids"
    use_choice="false"
    concealment="true" >
    <validator id="PartSet_DifferentObjects"/>
    <validator id="FeaturesPlugin_ValidatorBooleanSmashSelection"/>
  </multi_selector>
  <validator id="FeaturesPlugin_ValidatorBooleanArguments" parameters="main_objects,tool_objects"/>
</source>
